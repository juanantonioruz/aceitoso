<%=cargaeditor%>
<%=form_for [@museo,@vario],:html=>{:multipart=>true} do |f|%>

    <%=f.label :nombre,:class=>'label'%>
    <%=f.text_field :nombre%>  


    <%=f.label :descripcion,:class=>'label'%>
    <%=f.text_area :descripcion,:class=>'tinymce'%>
    
    <br/>
    <%=f.submit 'Guardar',:class=>'btn btn-success'%> o <%=link_to 'cancelar',museo_varios_path(@museo)%>
<%end%>
