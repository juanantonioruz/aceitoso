<%=form_for [@museo,@medio],:html=>{:multipart=>true} do |f|%>

    <%=f.label :nombre,:class=>'label'%>
    <%=f.text_field :nombre%>  


    <%=f.label :HTML_EMBED,:class=>'label'%>
    <%=f.text_area :descripcion%>
        <%=f.label 'marcar si tipo video, no marcar si tipo audio',:class=>'label'%>
  <%= f.radio_button :es_audio,  true %> NO
  <%= f.radio_button :es_audio,  false %> SI

    <br/>
    <%=f.submit 'Guardar',:class=>'btn btn-success'%> o <%=link_to 'cancelar',museo_medios_path(@museo)%>
<%end%>
